# Environment Variables Template
# Copy this file to .env and fill in your actual values

# SonarQube Configuration
SONAR_TOKEN=your_sonarqube_token_here
SONAR_HOST_URL=http://localhost:9000

# SonarQube Database (Internal)
SONAR_JDBC_URL=jdbc:postgresql://sonar_postgres:5432/sonar
SONAR_JDBC_USERNAME=sonar
SONAR_JDBC_PASSWORD=sonar
SONAR_ES_BOOTSTRAP_CHECKS_DISABLE=true

# MongoDB Configuration
# MongoDB Configuration
MONGO_INITDB_ROOT_USERNAME=admin
MONGO_INITDB_ROOT_PASSWORD=admin
MONGODB_URI=mongodb://admin:admin@mongodb:27017

# Superset Database (PostgreSQL)
POSTGRES_DB=superset
POSTGRES_USER=superset
POSTGRES_PASSWORD=superset

# SonarQube Database (PostgreSQL)
SONAR_POSTGRES_DB=sonar
SONAR_POSTGRES_USER=sonar
SONAR_POSTGRES_PASSWORD=sonar

# Superset Configuration
SUPERSET_SECRET_KEY=your_long_random_secret_key_here
SQLALCHEMY_DATABASE_URI=postgresql+psycopg2://superset:superset@superset_postgres:5432/superset
PREVENT_UNSAFE_DB_CONNECTIONS=False
SUPERSET_CONFIG_PATH=/app/superset_config.py

# Superset Admin User (from command)
SUPERSET_ADMIN_USERNAME=admin
SUPERSET_ADMIN_FIRSTNAME=Admin
SUPERSET_ADMIN_LASTNAME=User
SUPERSET_ADMIN_EMAIL=admin@admin.com
SUPERSET_ADMIN_PASSWORD=admin

# Development/Production Flag
ENVIRONMENT=development

# Trino Configuration
TRINO_HOST=localhost
TRINO_PORT=8080
